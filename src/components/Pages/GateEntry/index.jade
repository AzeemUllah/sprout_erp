                                    .col-xs-5.selectContainer
                                        select#check1.form-control.ship(name='Product')
                                            option(value='')
                                            option(value='Visitor') Visitor
                                            option(value='Customer') Customer
                                            option(value='Shipment') Shipment
                                            option(value='Vendor') Vendor

                    .col-md-6
                        .row
