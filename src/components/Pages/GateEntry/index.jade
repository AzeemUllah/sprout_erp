                                .form-group
                                    input.form-control.input-lg(type='numberValue')

                    .col-md-6
                        .row
                            .col-md-5
                                .cost
                                    label Contact Number
                            .col-xs-6.col-sm-3
                                .form-group
                                    input.form-control.input-lg(type='numberValue')
            .col-md-12
                .row
                    .col-md-6
                        .row
                            .col-md-5
                                label Company Name
                            .col-xs-6.col-sm-3
                                .input-group.dropdown
                                    input.form-control.countrycode.dropdown-toggle.create(type='text', value='')
                                    ul.dropdown-menu
                                        li
                                            a(href='') Create & Edit


                                    span.input-group-addon.dropdown-toggle(role='button', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false')
                                        span.caret


                    .col-md-6
                        .row
                            .col-md-5
                                label Entry Time
                            .col-xs-6.col-sm-3
