                    tr
                        td.o_td_label(colspan='1')
                            label.o_form_label(for='o_field_input_163')
                                | Header spacing
                        td(colspan='1', style='width: 100%;')
                            input#o_field_input_163.o_form_input.o_form_field.o_form_field_number(type='text')
                    tr
                        td.o_td_label(colspan='1')
                            label.o_form_label(for='o_field_input_164')
                                | Output DPI
                        td(colspan='1', style='width: 100%;')
                            input#o_field_input_164.o_form_input.o_form_field.o_form_required.o_form_field_number(type='text')
                    tr
                        td.o_td_label(colspan='1')
                            label.o_form_label.o_form_label_help(for='o_field_input_165', data-original-title='', title='')
                                | Associated reports
                        td(colspan='1', style='width: 100%;')
                            .o_form_field_many2manytags.o_form_field
                                .o_form_field_many2one.o_form_field(data-original-title='', title='')
                                    .o_form_input_dropdown
                                        input.o_form_input.ui-autocomplete-input(type='text', autocomplete='off')
                                        span.o_dropdown_button(draggable='false')
    .modal-footer
        button.btn.btn-sm.btn-primary(type='button')
